{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "guides/**.md",
          "guides/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "default(zh-cn)"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appTitle": "HuajiTech.Mirai 文档",
      "_gitUrlPattern": "github",
      "_enableSearch": true
    }
  }
}
